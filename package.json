  "bugs": {
    "url": "https://github.com/ILopatenko/test-task-shopltk/issues"
  },
  "homepage": "https://github.com/ILopatenko/test-task-shopltk#readme",
  "dependencies": {
    "cypress": "^9.6.0"
  }
}
